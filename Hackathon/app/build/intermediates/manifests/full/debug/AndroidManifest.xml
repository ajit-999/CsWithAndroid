<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pcsaini779.hackathon"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.pcsaini779.hackathon.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.pcsaini779.hackathon.Game" />
        <activity
            android:name="com.example.pcsaini779.hackathon.Instructions"
            android:label="@string/title_activity_instructions"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.pcsaini779.hackathon.Team"
            android:label="@string/title_activity_team"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>